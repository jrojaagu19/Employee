@page "/employees"

<h3>Empleados</h3>

@if ( employees == null)
{
    <p>Cargando. . .</p>
}
else
{
        <table border="1">
            <tr>
                <td> </td>
            </tr>
            @foreach (var employee in employees)
            {
                <tr>
                <td>@($"{employee.FirstName} {employee.LastName}")</td>
                </tr>
            }
        </table>
}